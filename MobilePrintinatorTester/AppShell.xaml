<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MobilePrintinatorTester.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MobilePrintinatorTester"
    xmlns:pages="clr-namespace:MobilePrintinatorTester.ContentPages"
    Title="MobilePrintinatorTester">

    <FlyoutItem Title="Testing">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:Testing}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Settings">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:Settings}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="About">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:About}" />
        </Tab>
    </FlyoutItem>

</Shell>
